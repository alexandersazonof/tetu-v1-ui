{
  "name": "tetu-v1-ui",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "dev": "yarn config:dev && ng serve",
    "config:dev": "ts-node ./scripts/setenv.ts --configuration=dev",
    "config:prod": "ts-node ./scripts/setenv.ts --configuration=prod",
    "prepare:dev": "yarn config:dev && yarn build:abi && yarn build:gql",
    "prepare:prod": "yarn config:prod && yarn build:abi && yarn build:gql",
    "build:dev": "yarn prepare:dev && ng build --configuration development",
    "build:prod": "yarn prepare:prod && ng build --configuration production",
    "build:abi": "typechain --target ethers-v5 ./abi/**/*.json --out-dir ./generated/abi",
    "build:gql": "graphql-codegen --config ./gql/codegen.yml",
    "ui:build": "yarn config:dev && ng build ui --configuration production",
    "ui:version:patch": "cd dist/ui && sleep 2 && npm version patch",
    "ui:publish": "npm login && cd dist/ui && npm publish --access=public",
    "ui:build:publish": "yarn ui:build && yarn ui:publish",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --no-watch --no-progress",
    "lint": "eslint \"src/**/*.ts\" && prettier -c ./src",
    "lint:fix": "eslint --fix \"src/**/*.ts\" && prettier -w ./src",
    "lint:fix:ui": "eslint --fix \"libs/ui/src/lib/**/*.ts\" && prettier -w libs/ui/src/lib",
    "heroku-postbuild": "yarn build:prod",
    "storybook": "start-storybook -p 6006"
  },
  "lint-staged": {
    "src/**/*.{ts,css,html}": [
      "npm run lint:fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.1.0",
    "@angular/common": "14.1.0",
    "@angular/compiler": "14.1.0",
    "@angular/core": "14.1.0",
    "@angular/forms": "14.1.0",
    "@angular/platform-browser": "14.1.0",
    "@angular/platform-browser-dynamic": "14.1.0",
    "@angular/router": "14.1.0",
    "@ant-design/icons": "4.7.0",
    "@apollo/client": "^3.7.9",
    "@ngx-translate/core": "14.0.0",
    "@tetu_io/tetu-ui": "1.0.28",
    "@typechain/ethers-v5": "10.1.0",
    "@walletconnect/web3-provider": "^1.8.0",
    "apollo-angular": "^4.2.1",
    "assert": "2.0.0",
    "browser": "0.2.6",
    "cors": "2.8.5",
    "crypto-browserify": "3.12.0",
